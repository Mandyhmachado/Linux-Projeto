#! /bin/bash

eco "Criando usuários do Sistema"

useradd guest10 -c"Usuário Convidado" -s /bin/bash -m -p$(openssl passwd - crypt senha321) senha guest10 -e

useradd guest11 -c"Usuário convidado" -s /bin/bash -m -p$(openssl passwd - crypt senha321) senha guest11 -e

useradd guest12 -c"Usuário convidado" -s /bin/bash -m -p$(openssl passwd - crypt senha321) senha guest12 -e

useradd guest13 -c"Usuário convidado" -s /bin/bash -m -p$(openssl passwd - crypt senha321) senha guest13 -e

eco "Finalizado"
